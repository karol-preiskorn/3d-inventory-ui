{
  "name": "3d-inventory-angular-ui",
  "version": "0.59.12",
  "private": true,
  "type": "module",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "buffer": false,
    "process": false
  },
  "scripts": {
    "build": "ng build 3d-inventory-angular-ui",
    "build:verbose": "ng build 3d-inventory-angular-ui --verbose",
    "build:prod": "ng build 3d-inventory-angular-ui --configuration=production",
    "build:analyze": "ANALYZE=true ng build 3d-inventory-angular-ui --configuration=production",
    "check:depcheck": "depcheck",
    "check:npm-check": "npm-check",
    "check:upgrade": "npm-check-updates -u && npm upgrade",
    "clear": "rimraf dist documentation nyc_output node_modules package-lock.json target",
    "compodoc:serve": "compodoc -p tsconfig.doc.json -s",
    "cp": "cpx README.md src/assets",
    "gcp:build": "npm run version:patch && ./build.sh",
    "docker:run": "docker run -p 4200:433 3d-inventory-ui",
    "kill-ng-serve": "fkill --force \"ng serve\"",
    "lint": "ng lint",
    "lint:check": "ng lint --max-warnings 0",
    "lint:errors-only": "ng lint | grep error",
    "lint:eslint": "eslint --fix src",
    "lint:eslint-check": "eslint src",
    "lint:prettier": "prettier --write src/**/*.ts",
    "lint:prettier-check": "prettier --check src/**/*.ts",
    "lint:stats": "npm run lint 2>&1 | grep problems || echo 'No linting problems found!'",
    "lint:fix-all": "./quick-lint-fix.sh",
    "pre-commit": "npm run lint && npm run lint:eslint && npm run version:minor",
    "security:check": "npm audit --audit-level high && npm run lint",
    "prestart": "npx kill-port 4200 && npm version patch --no-git-tag-version",
    "start": "npm run cp && ng serve",
    "test": "jest",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:patch": "npm version patch --no-git-tag-version",
    "watch": "npm --no-git-tag-version version patch && ng serve --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write --ignore-unknown",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.html": [
      "prettier --write --ignore-unknown",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "^20.3.2",
    "@angular/cdk": "^20.2.5",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.5",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@compodoc/compodoc": "^1.1.30",
    "@faker-js/faker": "^10.0.0",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@popperjs/core": "^2.11.8",
    "@typescript-eslint/parser": "^8.43.0",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "depcheck": "^1.4.7",
    "font-awesome": "^4.7.0",
    "jsdom": "^27.0.0",
    "ngx-pagination": "^6.0.3",
    "npm-check-updates": "^18.1.1",
    "prismjs": "^1.30.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "showdown": "^2.1.0",
    "uuid": "^13.0.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.1",
    "@angular/build": "^20.3.1",
    "@angular/cli": "^20.3.1",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/localize": "^20.3.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^9.14.0",
    "@types/jasmine": "~5.1.9",
    "@types/jest": "^30.0.0",
    "@types/karma-coverage": "^2.0.3",
    "@types/karma-jasmine": "^4.0.5",
    "@types/karma-jsdom-launcher": "^8.0.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.4.0",
    "@types/prismjs": "^1.26.5",
    "@types/showdown": "^2.0.6",
    "@types/three": "^0.180.0",
    "@types/uuid": "^10.0.0",
    "angular-cli-ghpages": "^2.0.3",
    "angular-eslint": "20.3.0",
    "babel-jest": "^30.1.2",
    "cpx": "^1.5.0",
    "eslint": "^9.35.0",
    "eslint-plugin-node": "11.1.0",
    "globals": "^16.4.0",
    "jasmine-core": "~5.10.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-preset-angular": "^15.0.1",
    "jest-runner-eslint": "^2.3.0",
    "karma": "~6.4.4",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-jsdom-launcher": "^19.0.0",
    "lint-staged": "^16.1.6",
    "nyc": "^17.1.0",
    "prettier": "3.6.2",
    "three": "^0.180.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "8.43.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "serve": {
    "options": {
      "proxyConfig": "src/proxy.conf.json"
    }
  }
}
