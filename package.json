{
  "name": "3d-inventory-angular-ui",
  "version": "0.41.0",
  "private": true,
  "type": "module",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "buffer": false,
    "process": false
  },
  "scripts": {
    "build": "ng build 3d-inventory-angular-ui --verbose",
    "clear": "rm -rf dist documentation ./nyc_output ./node_modules ./package-lock.json ./target",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "cp": "cp -f README.md src/assets",
    "eslint": "eslint --fix src",
    "kill-ng-serve": "killall -w -v \"ng serve\"",
    "lint": "ng lint",
    "lint:eslint": "eslint --fix src",
    "lint:prettier": "prettier --write src/**/*.ts",
    "pre-commit": "lint-staged",
    "prestart": "(fuser -n tcp -k 4200)&",
    "start": "npm run cp && ng serve --watch",
    "test": "jest",
    "upgrade-full": "npm-check-updates -u && npm upgrade",
    "watch": "npm --no-git-tag-version version patch && ng serve --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write --ignore-unknown src/**/*.ts",
      "eslint --fix src",
      "git add -A ."
    ],
    "src/**/*.html": [
      "prettier --write --ignore-unknown src/**/*.ts",
      "git add -A ."
    ]
  },
  "dependencies": {
    "@angular/animations": "^19.2.9",
    "@angular/common": "^19.2.9",
    "@angular/compiler": "^19.2.9",
    "@angular/core": "^19.2.9",
    "@angular/forms": "^19.2.9",
    "@angular/platform-browser": "^19.2.9",
    "@angular/platform-browser-dynamic": "^19.2.9",
    "@angular/platform-server": "^19.2.9",
    "@angular/router": "^19.2.9",
    "@compodoc/compodoc": "^1.1.26",
    "@faker-js/faker": "^9.7.0",
    "@jest/globals": "^29.7.0",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@octokit/core": "^6.1.5",
    "bootstrap": "^5.3.5",
    "bootstrap-icons": "^1.11.4",
    "browserify-zlib": "^0.2.0",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "depcheck": "^1.4.7",
    "eslint-scope": "^8.3.0",
    "express": "^5.1.0",
    "font-awesome": "^4.7.0",
    "https-browserify": "^1.0.0",
    "install": "^0.13.0",
    "jquery": "^3.7.1",
    "jsdom": "^26.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "mongodb": "^6.16.0",
    "nav-bar": "^1.0.3",
    "ngx-markdown": "^19.1.1",
    "node-gyp": "^11.2.0",
    "node-sass": "^9.0.0",
    "npm-check-updates": "^18.0.1",
    "octokit": "^5.0.2",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.30.0",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "rxjs": "~7.8.2",
    "stream": "^0.0.3",
    "stream-browserify": "^3.0.0",
    "three-orbitcontrols-ts": "^0.1.2",
    "timers": "^0.1.1",
    "timers-browserify": "^2.0.12",
    "ts-sync-request": "^1.4.1",
    "tslib": "^2.8.1",
    "winston": "^3.17.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.1",
    "@angular-devkit/build-angular": "^19.2.10",
    "@angular-eslint/builder": "^19.3.0",
    "@angular-eslint/eslint-plugin": "^19.3.0",
    "@angular-eslint/eslint-plugin-template": "^19.3.0",
    "@angular-eslint/schematics": "^19.3.0",
    "@angular-eslint/template-parser": "^19.3.0",
    "@angular/cli": "^19.2.10",
    "@angular/compiler-cli": "^19.2.9",
