{
  "name": "3d-inventory-angular-ui",
  "version": "0.59.0",
  "private": true,
  "type": "module",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "buffer": false,
    "process": false
  },
  "scripts": {
    "build": "ng build 3d-inventory-angular-ui --verbose",
    "check:depcheck": "npx depcheck",
    "check:npm-check": "npx npm-check",
    "check:upgrade": "npx npm-check-updates -u && npm upgrade",
    "clear": "rm -rf dist documentation ./nyc_output ./node_modules ./package-lock.json ./target",
    "compodoc:serve": "npx compodoc -p tsconfig.doc.json -s",
    "cp": "cpx README.md src/assets",
    "docker:build": "npm run version:patch && ./build.sh",
    "docker:run": "docker run -p 4200:433 3d-inventory-ui",
    "kill-ng-serve": "fkill --force \"ng serve\"",
    "lint": "ng lint",
    "lint:eslint": "eslint --fix src",
    "lint:prettier": "prettier --write src/**/*.ts",
    "pre-commit": "npm run lint && npm run lint:eslint && npm run version:minor",
    "security:check": "npm audit --audit-level high && npm run lint",
    "prestart": "npx kill-port 4200 && npm version patch --no-git-tag-version",
    "start": "npm run cp && ng serve",
    "test": "jest",
    "upgrade-full": "npm-check-updates -u && npm upgrade",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:patch": "npm version patch --no-git-tag-version",
    "watch": "npm --no-git-tag-version version patch && ng serve --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write --ignore-unknown src/**/*.ts",
      "eslint --fix src",
      "git add -A ."
    ],
    "src/**/*.html": [
      "prettier --write --ignore-unknown src/**/*.html",
      "git add -A ."
    ]
  },
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/platform-browser-dynamic": "^20.1.3",
    "@angular/platform-server": "^20.1.3",
    "@angular/router": "^20.1.3",
    "@compodoc/compodoc": "^1.1.26",
    "@faker-js/faker": "^9.9.0",
    "@jest/globals": "^29.0.0",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@octokit/core": "^7.0.3",
    "@popperjs/core": "^2.11.8",
    "@typescript-eslint/parser": "^8.38.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "browserify-zlib": "^0.2.0",
    "depcheck": "^1.4.7",
    "firebase": "^12.0.0",
    "font-awesome": "^4.7.0",
    "jsdom": "^26.1.0",
    "npm-check-updates": "^18.0.2",
    "prismjs": "^1.30.0",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "rxjs": "~7.8.2",
    "showdown": "^2.1.0",
    "stream": "^0.0.3",
    "tslib": "^2.8.1",
    "winston": "^3.17.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.3",
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.3",
    "@angular/localize": "^20.1.3",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^9.14.0",
    "@types/jasmine": "~5.1.8",
    "@types/jest": "^29.0.0",
    "@types/karma-coverage": "^2.0.3",
    "@types/karma-jasmine": "^4.0.5",
    "@types/karma-jsdom-launcher": "^8.0.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.1.0",
    "@types/prismjs": "^1.26.5",
    "@types/showdown": "^2.0.6",
    "@types/three": "^0.178.1",
    "@types/uuid": "^10.0.0",
    "angular-cli-ghpages": "^2.0.3",
    "angular-eslint": "20.1.1",
    "babel-jest": "^29.0.0",
    "cpx": "^1.5.0",
    "eslint": "^9.31.0",
    "eslint-plugin-node": "11.1.0",
    "globals": "^16.3.0",
    "jasmine-core": "~5.9.0",
    "jest": "^29.0.0",
    "karma": "~6.4.4",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-jsdom-launcher": "^18.0.0",
    "lint-staged": "^16.1.2",
    "nyc": "^17.1.0",
    "prettier": "3.6.2",
    "three": "^0.178.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.38.0"
  },
  "serve": {
    "options": {
      "proxyConfig": "src/proxy.conf.json"
    }
  }
}
