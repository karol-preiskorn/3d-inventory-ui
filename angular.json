{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "3d-inventory-angular-ui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "sourceMap": true,
            "outputPath": "dist",
            "index": "./src/index.html",
            "main": "./src/main.ts",
            "polyfills": "./src/polyfills.ts",
            "tsConfig": "./tsconfig.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "./src/favicon.ico",
              "./src/assets",
              {
                "glob": "*.svg",
                "input": "./node_modules/bootstrap-icons/",
                "output": "/"
              }
            ],
            "styles": [
              "./src/styles.scss",
              "./node_modules/bootstrap/scss/bootstrap.scss",
              "./node_modules/bootstrap-icons/font/bootstrap-icons.scss",
              "./node_modules/prismjs/themes/prism-okaidia.css",
              "./node_modules/font-awesome/css/font-awesome.min.css"
            ],
            "scripts": [
              "./node_modules/bootstrap/dist/js/bootstrap.bundle.js",
              "./node_modules/marked/marked.min.js",
              "./node_modules/prismjs/plugins/line-numbers/prism-line-numbers.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5kb",
                  "maximumError": "10kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "./src/environments/environment.ts",
                  "with": "./src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "optimization": {
              "buildOptimizer": true,
              "optimization": true,
              "vendorChunk": false,
              "extractLicenses": true,
              "sourceMap": false,
              "namedChunks": false
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "buildTarget": "3d-inventory-angular-ui:build:development",
            "proxyConfig": "src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "3d-inventory-angular-ui:build:production"
            },
            "development": {
              "buildTarget": "3d-inventory-angular-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "3d-inventory-angular-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "./src/favicon.ico",
              "./src/assets",
              {
                "glob": "*.svg",
                "input": "./node_modules/bootstrap-icons/",
                "output": "/"
              }
            ],
            "styles": [
              "./src/styles.scss",
              "./node_modules/bootstrap/scss/bootstrap.scss",
              "./node_modules/bootstrap-icons/font/bootstrap-icons.scss",
              "./node_modules/prismjs/themes/prism-okaidia.css"
            ],
            "scripts": [
              "./node_modules/bootstrap/dist/js/bootstrap.bundle.js",
              "./node_modules/marked/marked.min.js",
              "./node_modules/prismjs/plugins/line-numbers/prism-line-numbers.js"
            ]
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "./src/**/*.ts",
              "./src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "5eb5f275-1cef-4565-aeac-442bad6845a0",
    "cache": {
      "enabled": false
    },
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
